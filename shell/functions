function gifit {
  ruby ~/.dotfiles/dev_scripts/screengif/screengif.rb --input $1 --output $2
}

function notes {
  code $DROPBOX/Notes/$1
}

function feat {
  git checkout -b feature/$1
}

function fix {
  git checkout -b fix/$1
}

function chore {
  git checkout -b chore/$1
}

function refactor {
  git checkout -b refactor/$1
}

function build {
  git checkout -b build/$1
}
